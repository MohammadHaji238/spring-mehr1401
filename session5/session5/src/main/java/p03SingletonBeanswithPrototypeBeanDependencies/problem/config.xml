<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
">






    <bean id="singleton" class="p03SingletonBeanswithPrototypeBeanDependencies.problem.SingletonScope" scope="singleton" p:prototypeBean-ref="prototype"/>
    <bean id="prototype" class="p03SingletonBeanswithPrototypeBeanDependencies.problem.PrototypeScope" scope="prototype">
        <property name="message" value="Test Message" />
    </bean>
</beans>